@model WorldIndicator.Models.Country
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>AcidLab Test</title>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/datepicker")
    @Styles.Render("~/Content/bootstrap")
    @Styles.Render("~/Content/css")

</head>
<body>
        <script src="~/Scripts/jquery.dataTables.min.js"></script>
        <link rel="stylesheet" type="text/css" href="~/Content/jquery.dataTables.min.css"> 


    @using (Html.BeginForm("", "", FormMethod.Post))
    {

        //var items = new List<SelectListItem>()
        //    {
        //        new SelectListItem() { Text = "Item 1", Value = "#" },
        //        new SelectListItem() { Text = "Item 2", Value = "#" },
        //    };
        SelectList list = ViewBag.list;

        <form role="form" action="" method="post">
            <div class="panel panel-default panel-table">
                <div class="panel-heading">
                    <div class="row">
                        <div class="col-lg-6">
                            <div class="form-group">
                                @Html.Label("Country :", new { @class = "", style = "font-size:medium" })
                                <div class="input-group">
                                    @Html.DropDownListFor(Model => Model.CountryCode, list, new { @class = "form-control", style = "width:200px" })
                                </div>
                            </div>
                            <div class="form-group">
                                @Html.Label("Start Date :", new { @class = "", style = "font-size:medium" })
                                <div class="input-group">
                                    <input id="StartDate" class="datepicker" size="46" type="text">
                                </div>
                            </div>
                            <div class="form-group">
                                @Html.Label("End Date :", new { @class = "", style = "font-size:medium" })
                                <div class="input-group">
                                    <input id="EndDate" class="datepicker" size="46" type="text">
                                </div>
                            </div>
                            <div class="form-group">
                                <button type="button" id="Process" class="btn btn-default">Process</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel panel-default panel-table">
                <div class="panel-body">
                    <div id="tableresult">
                        <table id="location" class="table table-striped table-bordered datatable">
                            <thead>
                                <tr>
                                    <th>CountryName</th>
                                    <th>Rate</th>
                                    <th>Year</th>
                                    <th>Description</th>
                                </tr>
                            </thead>
                            <tbody>

                            </tbody>
                        </table>

                    </div>
                </div>
            </div>


        </form>
    }
</body>
</html>


